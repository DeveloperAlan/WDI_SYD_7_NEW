<%= form_for  @artist do |f| %>

<div class="form-group">
  <%= f.label :name%>
  <%= f.text_field :name%><br>
</div>
<div class="form-group">
  <%= f.label :hometown %>
  <%= f.text_field :hometown %>
</div>
<div class="form-group">
  <%= f.label :bio %>
  <%= f.text_field :bio %>
</div>
<div class="form-group">
  <%= f.label :years_active%>
  <%= f.text_field :years_active%>
</div>
<div class="form-group">
  <%= f.label :label%>
  <%= f.text_field :label%>
</div>
<div class="form-group">
  <%= f.submit %>
</div>

<% end %>